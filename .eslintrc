{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"standard-with-typescript",
		"prettier"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module",
		"project": ["./tsconfig.json"]
	},
	"plugins": ["react"],
	"ignorePatterns": ["dist"],
	"rules": {
		"@typescript-eslint/explicit-function-return-type": "off"
	}
}
